#include <stdio.h> 
#include<stdbool.h>

int bubbleSort(int arr[], int n,int *c1) 
{ 
   int i, j,temp,t=0; 
     bool sw;
   for (i = 0; i < n-1; i++)       
   {
        sw=false;
       for (j = 0; j < n-i-1; j++)  
       {t++;
           if (arr[j] > arr[j+1]) 
    {
        temp=arr[j];
        arr[j]=arr[j+1];
        arr[j+1]=temp;
               sw=true;
               (*c1)++;
               
    }
       }
       if(sw==false)
           break;
   }
return t;   
} 

  
void printArray(int arr[], int size) 
{ 
    int i; 
    for (i=0; i < size; i++) 
        printf("%d ", arr[i]); 
    printf("\n"); 
} 
  
int main() 
{ 
    int arr[100],i ,n,t,c=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d",&arr[i]);
    t=bubbleSort(arr, n,&c); 
    printArray(arr, n); 
    printf("%d",t);
    printf("\n%d",c);
    return 0;
}
  
